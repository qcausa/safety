#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/browser-sync@3.0.2/node_modules/browser-sync/dist/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/browser-sync@3.0.2/node_modules/browser-sync/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/browser-sync@3.0.2/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/browser-sync@3.0.2/node_modules/browser-sync/dist/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/browser-sync@3.0.2/node_modules/browser-sync/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/browser-sync@3.0.2/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../browser-sync/dist/bin.js" "$@"
else
  exec node  "$basedir/../browser-sync/dist/bin.js" "$@"
fi
