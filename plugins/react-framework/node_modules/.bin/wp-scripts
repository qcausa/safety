#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/@wordpress+scripts@27.9.0_@playwright+test@1.47.0_react-dom@18.3.1_react@18.3.1_typescript@5.6.2/node_modules/@wordpress/scripts/bin/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/@wordpress+scripts@27.9.0_@playwright+test@1.47.0_react-dom@18.3.1_react@18.3.1_typescript@5.6.2/node_modules/@wordpress/scripts/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/@wordpress+scripts@27.9.0_@playwright+test@1.47.0_react-dom@18.3.1_react@18.3.1_typescript@5.6.2/node_modules/@wordpress/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/@wordpress+scripts@27.9.0_@playwright+test@1.47.0_react-dom@18.3.1_react@18.3.1_typescript@5.6.2/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/@wordpress+scripts@27.9.0_@playwright+test@1.47.0_react-dom@18.3.1_react@18.3.1_typescript@5.6.2/node_modules/@wordpress/scripts/bin/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/@wordpress+scripts@27.9.0_@playwright+test@1.47.0_react-dom@18.3.1_react@18.3.1_typescript@5.6.2/node_modules/@wordpress/scripts/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/@wordpress+scripts@27.9.0_@playwright+test@1.47.0_react-dom@18.3.1_react@18.3.1_typescript@5.6.2/node_modules/@wordpress/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/@wordpress+scripts@27.9.0_@playwright+test@1.47.0_react-dom@18.3.1_react@18.3.1_typescript@5.6.2/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@wordpress/scripts/bin/wp-scripts.js" "$@"
else
  exec node  "$basedir/../@wordpress/scripts/bin/wp-scripts.js" "$@"
fi
