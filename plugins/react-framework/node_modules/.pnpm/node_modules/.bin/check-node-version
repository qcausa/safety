#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/check-node-version@4.2.1/node_modules/check-node-version/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/check-node-version@4.2.1/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/check-node-version@4.2.1/node_modules/check-node-version/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/check-node-version@4.2.1/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../check-node-version/bin.js" "$@"
else
  exec node  "$basedir/../check-node-version/bin.js" "$@"
fi
