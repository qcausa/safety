#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/lighthouse@10.4.0_typescript@5.6.2/node_modules/lighthouse/cli/test/smokehouse/frontends/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/lighthouse@10.4.0_typescript@5.6.2/node_modules/lighthouse/cli/test/smokehouse/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/lighthouse@10.4.0_typescript@5.6.2/node_modules/lighthouse/cli/test/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/lighthouse@10.4.0_typescript@5.6.2/node_modules/lighthouse/cli/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/lighthouse@10.4.0_typescript@5.6.2/node_modules/lighthouse/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/lighthouse@10.4.0_typescript@5.6.2/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/lighthouse@10.4.0_typescript@5.6.2/node_modules/lighthouse/cli/test/smokehouse/frontends/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/lighthouse@10.4.0_typescript@5.6.2/node_modules/lighthouse/cli/test/smokehouse/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/lighthouse@10.4.0_typescript@5.6.2/node_modules/lighthouse/cli/test/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/lighthouse@10.4.0_typescript@5.6.2/node_modules/lighthouse/cli/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/lighthouse@10.4.0_typescript@5.6.2/node_modules/lighthouse/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/lighthouse@10.4.0_typescript@5.6.2/node_modules:/home/desitdt/Local Sites/safetyportal2/app/public/wp-content/plugins/react-framework/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lighthouse/cli/test/smokehouse/frontends/smokehouse-bin.js" "$@"
else
  exec node  "$basedir/../lighthouse/cli/test/smokehouse/frontends/smokehouse-bin.js" "$@"
fi
